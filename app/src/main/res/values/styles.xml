<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="buttons_back">
        <item name="android:background">@drawable/buttons_style</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:color">#DFDFDE</item>
    </style>

    <style name="button_sos_back">
        <item name="android:background">@drawable/buttons_sos_style</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:color">#DFDFDE</item>
    </style>

</resources>
